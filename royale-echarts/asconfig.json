/*
 * https://github.com/BowlerHatLLC/vscode-as3mxml/wiki/asconfig.json#library-path
 */
 {
    "config": "royale",
    "type": "lib",
    "compilerOptions": {
        "debug": true,
        "targets": [
            // "SWF",
            "JSRoyale"
        ],
        "namespace": [
            {
                "uri": "library://ns.apache.org/royale/community/echarts",
                "manifest": "src/main/resources/echarts-manifest.xml"
            }
        ],
        "define": [
            {
                "name": "COMPILE::JS",
                "value": true
            },
            {
                "name": "COMPILE::SWF",
                "value": false
            }
        ],
        "source-path": [
            "src/main/royale"
        ],
        "include-classes": [
            "src/main/royale/ECHARTSClasses.as"
        ],
        "include-sources": [
            "src/main/royale"
        ],
        "source-map": true,
        "output": "target/royale-echarts-1.0-SNAPSHOT-js.swc"
    }
}
