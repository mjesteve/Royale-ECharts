 {
    "config": "royale",
    "type": "lib",
    "compilerOptions": {
        "debug": true,
        "targets": [
            // "SWF",
            "JSRoyale"
        ],
        "namespace": [
            {
                "uri": "library://ns.apache.org/royale/echarts",
                "manifest": "src/main/resources/echarts-manifest.xml"
            }
        ],
        "define": [
            {
                "name": "COMPILE::JS",
                "value": true
            },
            {
                "name": "COMPILE::SWF",
                "value": false
            }
        ],
        "source-path": [
            "src/main/royale"
        ],
        "include-classes": [
            "ChartsClasses"
        ],
        "include-sources": [
            "src/main/royale"
        ],
        // "library-path": [
        //     "${royalelib}/js/libs/MXRoyaleJS.swc"
        // ],
        // "js-library-path": [
        //     "${royalelib}/js/libs/MXRoyaleJS.swc"
        // ],
        "source-map": true,
        "output": "target/royale-echarts-1.0-SNAPSHOT-js.swc"
    }
}
{
