		private function convertData(data:Array):Array {
            var res:Array = [];
            for (var i:int = 0; i < data.length; i++) {
                var geoCoord:Array = geoCoordMap[data[i].name];
                if (geoCoord) {
                    if (!data[i].value == '' && data[i].value > 0) {
                        res.push({
                            name: data[i].name,
                            value: geoCoord.concat(data[i].value)
                        });
                    }
                }
            }
            return res;
        }

